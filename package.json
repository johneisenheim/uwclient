{
  "name": "material-kit-react",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "1.0.0-beta.43",
    "apollo-boost": "^0.1.6",
    "axios": "^0.18.0",
    "graphql": "^0.13.2",
    "history": "4.7.2",
    "material-ui": "1.0.0-beta.45",
    "moment": "2.22.1",
    "node-sass-chokidar": "1.2.2",
    "npm-run-all": "4.1.2",
    "polyglot-react": "^1.0.1",
    "react": "16.3.1",
    "react-apollo": "^2.1.4",
    "react-cookie": "^2.1.5",
    "react-datetime": "2.14.0",
    "react-dom": "16.3.1",
    "react-nouislider": "2.0.1",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.4",
    "react-slick": "0.23.1",
    "react-swipeable-views": "0.12.13",
    "react-text-more-less": "^0.3.0",
    "react-textarea-autosize": "^6.1.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start:test": "NODE_ENV=test npm-run-all -p watch-css start-js",
    "start:prod": "NODE_ENV=prod npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
